					<input type="radio" name="sampleradio" value="3"> Radio 3
				</label>
				<label><input type="radio" name="sampleradio2" value="1"> Radio 4</label>
				<label><input type="radio" name="sampleradio2" value="2" checked="checked"> Radio 5</label>
				<label><input type="radio" name="sampleradio2" value="3"> Radio 6</label>
				<label for="">File</label>
				<input type="file">
				<label for="">Button</label>
				<button>Sample Button</button>
				<br>
				<br>
				<label for="">Submit</label>
				<input type="submit">
				<br/>
