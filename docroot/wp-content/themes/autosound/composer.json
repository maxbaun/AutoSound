		"asm89/twig-cache-extension" : "~1.0"
    },
	"require-dev": {
	  "squizlabs/php_codesniffer": "3.0.2",
      "sirbrillig/phpcs-variable-analysis": "2.0.7",
      "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
	},
    "extra": {
		"installer-paths": {
			"plugins/{$name}" : ["type:wordpress-plugin"]
		}
    }
}
